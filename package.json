{
  "homepage": "https://belle1306.github.io/infinity/",
  "name": "infinity",
  "version": "0.0.0",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "private": true,
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon ./bin/www",
    "build": "cd client/ && npm run build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "migrate": "node model/database.js"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "deployment": "^0.2.3",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.7",
    "react-datepicker": "^3.4.1"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0"
  }
}
